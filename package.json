{
  "name": "node-locator",
  "version": "1.0.1",
  "description": "A simple dependency injection container/locator for nodejs based on config files",
  "main": "index.cjs.js",
  "module": "index.esm.js",
  "type": "module",
  "exports": {
    ".": {
      "require": "./index.cjs",
      "import": "./index.esm.js"
    }
  },
  "scripts": {
    "test": "node_modules/.bin/mocha test --recursive"
  },
  "files": [
    "index.cjs",
    "index.esm.js",
    "locator.cjs",
    "locator.esm.js",
    "locator_factory.cjs",
    "locator_factory.esm.js"
  ],
  "author": "Ruud Arentsen",
  "license": "MIT",
  "dependencies": {
    "config": "^3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/roed/node-locator.git"
  },
  "devDependencies": {
    "mocha": "^10.7.3"
  }
}
